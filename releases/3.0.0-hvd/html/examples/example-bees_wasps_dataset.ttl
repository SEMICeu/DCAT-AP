@prefix : <http://www.example.org/rdf#>
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

:dataset_bees a dcat:Dataset;
  dct:title "The population of bees"@en;
  dcatap:applicableLegislation <http://data.europa.eu/eli/reg_impl/2023/138/oj>,
    <http://data.europa.eu/eli/dir/2007/2/2019-06-26>;
  dcatap:hvdCategory <http://data.europa.eu/bna/c_b7f6a4f3>;
  dct:description "This dataset provides the population of bees based on a yearly inquiry held among beekeepers and farmers in spring."@en .

:dataset_wasp a dcat:Dataset;
  dct:title "The population of wasps"@en;
  dcatap:applicableLegislation <http://data.europa.eu/eli/dir/2007/2/2019-06-26>;
  dct:description "This dataset provides the population of wasps based on a yearly inquiry held among beekeepers, farmers, firefighters and catering industry in august."@en .
