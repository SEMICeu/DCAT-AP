 <!-- http://data.europa.eu/r5r/availability/1.0-->
   <skos:ConceptScheme rdf:about="http://data.europa.eu/r5r/availability/1.0">
    <rdfs:label xml:lang="en">List of availability levels</rdfs:label>
    <vann:usageNote xml:lang="en">The list of terms for the avalability levels of a dataset distribution  in the DCAT-AP  specification.</vann:usageNote>
    <skos:prefLabel xml:lang="en">List of availability levels</skos:prefLabel>
    <dct:identifier>http://data.europa.eu/r5r/availability/1.0</dct:identifier>
    <skos:hasTopConcept rdf:resource="http://data.europa.eu/r5r/availability/temporary"/>
    <skos:hasTopConcept rdf:resource="http://data.europa.eu/r5r/availability/experimental"/>
    <skos:hasTopConcept rdf:resource="http://data.europa.eu/r5r/availability/available"/>
    <skos:hasTopConcept rdf:resource="http://data.europa.eu/r5r/availability/stable"/>
  </skos:ConceptScheme>

  <skos:Concept rdf:about="http://data.europa.eu/r5r/availability/temporary">
    <dct:identifier>temporary</dct:identifier>
    <skos:inScheme rdf:resource="http://data.europa.eu/r5r/availability/1.0"/>
    <skos:prefLabel xml:lang="en">temporary</skos:prefLabel>
    <skos:definition xml:lang="en">Data can disappear at any time, no planning.</skos:definition>
  </skos:Concept>

  <skos:Concept rdf:about="http://data.europa.eu/r5r/experimental">
    <dct:identifier>experimental</dct:identifier>
    <skos:inScheme rdf:resource="http://data.europa.eu/r5r/availability/1.0"/>
    <skos:prefLabel xml:lang="en">experimental</skos:prefLabel>
    <skos:definition xml:lang="en">Data is available for a few years, medium term planning.</skos:definition>
  </skos:Concept>

  <skos:Concept rdf:about="http://data.europa.eu/r5r/available">
    <dct:identifier>available</dct:identifier>
    <skos:inScheme rdf:resource="http://data.europa.eu/r5r/availability/1.0"/>
    <skos:prefLabel xml:lang="en">available</skos:prefLabel>
    <skos:definition xml:lang="en">Data is available for a few years, medium term planning.</skos:definition>
  </skos:Concept>

  <skos:Concept rdf:about="http://data.europa.eu/r5r/stable">
    <dct:identifier>stable</dct:identifier>
    <skos:inScheme rdf:resource="http://data.europa.eu/r5r/availability/1.0"/>
    <skos:prefLabel xml:lang="en">stable</skos:prefLabel>
    <skos:definition xml:lang="en">Data will remain available in the long term.</skos:definition>
  </skos:Concept>


