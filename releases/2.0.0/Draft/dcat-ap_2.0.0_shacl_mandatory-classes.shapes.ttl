:MandatoryAgent
  rdf:type sh:NodeShape ;
  rdfs:comment "Mandatory agent " ;
  rdfs:label "Mandatory agent" ;
  sh:property [
      sh:minCount 1 ;
      sh:path [
          sh:inversePath rdf:type ;
        ] ;
    ] ;
  sh:targetClass "foaf:Agent" ;
.
:MandatoryCatalog
  rdf:type sh:NodeShape ;
  rdfs:comment "Mandatory catalog" ;
  rdfs:label "Mandatory catalog" ;
